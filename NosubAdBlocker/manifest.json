    {
      "matches": ["*://www.nosub.tv/*"],
      "css"    : [
        "content_scripts/css/ad_block.css"
      ],
      "js"     : [
      ],
      "run_at" : "document_idle",
      "all_frames": false
    },
    {
      "matches": ["*://www.nosub.tv/channel/*"],
      "css"    : [
      ],
      "js"     : [
        "content_scripts/js/ad_block.js"
      ],
      "run_at" : "document_start",
      "all_frames": false
    }
  ],
  "permissions": [
    "*://www.nosub.tv/*"
  ],
  "web_accessible_resources": [
    "vendor/js/*.map"
  ]
}

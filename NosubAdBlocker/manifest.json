{
  "name": "Nosub Ad Blocker",
  "description": "Blocking nosub's ads",
  "author": "KONO Shizuku",
  "icons": {
    "128": "assets/icons/icon128.png",
    "48": "assets/icons/icon48.png"
  },
  "version": "1.0.1.0",
  "manifest_version": 2,
  "background": {
  },
  "content_scripts": [
    {
    "matches": ["http://www.nosub.tv/*"],
    "css"    : [
      "content_scripts/css/ad_block.css"
    ],
    "js"     : [
    ],
    "run_at" : "document_idle",
    "all_frames": false
    }
  ],
  "permissions": [
    "http://www.nosub.tv/*"
  ],
  "web_accessible_resources": [
    "vendor/js/*.map"
  ]
}
